{
  "name": "tokensearch.js",
  "version": "0.5.15",
  "description": "Substring token search",
  "main": "./lib/tokensearch.min.js",
  "author": {
    "name": "Michael Vogt",
    "email": "michael@neeo.com"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/neophob/tokensearch.js.git"
  },
  "keywords": [
    "string search",
    "token",
    "substring",
    "collection"
  ],
  "license": "MIT",
  "devDependencies": {
    "mocha": "~2.2.1",
    "chai": "~2.1.2",
    "uglify-js": "~2.4.17"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "bugs": {
    "url": "https://github.com/neophob/tokensearch.js/issues"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter list && npm run-script minify",
    "clean": "rm ./lib/tokensearch.min.js",
    "minify": "node_modules/uglify-js/bin/uglifyjs lib/tokensearch.js --screw-ie8 --stats --lint --compress --mangle -o lib/tokensearch.min.js",
    "gitcommit": "git commit -am 'update minified code' | cat && git push | cat",
    "release": "npm test && npm run-script gitcommit && npm version patch -m 'Upgrade version to %s' && npm publish && git push --tags",
    "bowerrelease": "bower version patch -m 'Upgrade version to %s'"
  }
}
